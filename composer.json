{
    "name": "ronappleton/radmin-tinymce",
    "description": "TinyMce Integration for Radmin",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ron Appleton",
            "email": "ron.appleton@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "RonAppleton\\Radmin\\TinyMce\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RonAppleton\\Radmin\\TinyMce\\ModuleServiceProvider"
            ],
            "aliases": {
                "TinyMce": "RonAppleton\\Radmin\\TinyMce\\Facades\\TinyMceFacade"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
